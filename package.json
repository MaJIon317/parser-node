{
  "name": "fetch-server",
  "version": "1.0.0",
  "description": "Node.js fetch server using puppeteer-extra + stealth to fetch pages and attempt Cloudflare/Turnstile bypass; supports proxy-chain, cookie storage and geo/user-agent rotation.",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --quiet server.js",
    "lint": "eslint . --ext .js"
  },
  "keywords": [
    "puppeteer",
    "puppeteer-extra",
    "stealth",
    "cloudflare",
    "turnstile",
    "proxy-chain",
    "scraping",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18 <25"
  },
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^24.6.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "proxy-chain": "^1.0.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "eslint": "^9.39.1"
  }
}
